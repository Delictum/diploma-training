procedure TForm4.BitBtn1Click(Sender: TObject);
begin
    Form1.Close;
    Form2.Close;
    Form3.Close;
    Form4.Close;
end;

procedure TForm4.Button1Click(Sender: TObject);
begin
O5:=false;
end;

procedure TForm4.Button2Click(Sender: TObject);
begin
O5:=false;
end;

