Program Est; 
var
  i, N, F: integer; {Обозначаю переменные}
begin
  write('Введите N = ');
  read(N); {Ввожу переменную N}
  for i := 1 to N do {Задаю цикл: от счётика 1 до N выполнить следующее действие}
  begin
    F:=(2 * i - 1)-(i-1); {Переменная F выполняет выражение по циклу: (2 * счётчик i - 1)-(счётик i-1) }
    writeln(sqr(F)); {Вывод переменной F в квадрате}
  end;
end.